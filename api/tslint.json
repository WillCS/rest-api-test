{
    "extends": [
        "tslint:recommended"
    ],
    "defaultSeverity": "warning",
    "linterOptions": {
      "exclude": [
        "node_modules/**/*.ts"
      ]
    },
    "rules": {
      "whitespace": false,
      "trailing-comma": false,
      "interface-over-type-literal": false,
      "quotemark": false,
      "arrow-parens": [true, "ban-single-arg-parens"],
      "interface-name": false,
      "indent": [true, "spaces"],
      "object-literal-sort-keys": false
    }
  }
  